<tr class="account_tran_fields">
  <td>
    <%= f.collection_select :account_id, @accounts, :id, :name, { include_blank: true }, class: "chosen_select" %>
  </td>

  <td>
    <% f.object.amount *= -1 if dec && f.object.amount %>
    <%= f.number_field :amount, placeholder: "0.00", value: number_with_precision(f.object.amount, precision: 2), step: 0.01, class: 'account_tran_amount' %>
  </td>

  <td>
    <%= f.text_field :memo %>
  </td>

  <td>
    <%= f.collection_select :property_id, @properties, :id, :full_address, { include_blank: true }, class: "chosen_select" %>
  </td>

  <td>
    <%= f.hidden_field :_destroy %>
    <a href="#" class="remove_account_tran"><i class="fa fa-remove"></i></a>
  </td>
</tr>

