<%= render 'header' %>

<%= render 'shared/search', path: customers_path %>

<table class="table table-hover row-links">
  <thead class="thead-default">
    <tr>
      <th>Name</th>
      <th>Property</th>
      <th>Balance</th>
      <th>Phone</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <%= render @customers %>
  </tbody>
</table>

<%= will_paginate %>
